{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": ["http://localhost:4173/"]
    },
    "assert": {
      "assertions": {
        "categories:accessibility": "error",
        "categories:performance": "warn",
        "categories:seo": "error"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}